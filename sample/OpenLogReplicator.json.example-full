{
  "version": "0.7.10",
  "dump-redo-log": 0,
  "dump-raw-data": 0,
  "trace": 0,
  "trace2": 0,
  "sources": [
    {
      "alias": "S1",
      "name": "O112A",
      "reader": {
        "type": "online", 
        "path-mapping": ["/db/fra", "/opt/fast-recovery-area"],
        "user": "user1",
        "password": "Password1",
        "server": "//host:1521/SERVICE",
        "disable-checks": 0
      },
      "format": {
        "type": "json",
        "timestamp-format": 0, 
        "char-format": 0,
        "scn-format": 0,
        "unknown-format": 0,
        "metadata": 0,
        "show-columns": 0
      },
      "arch": "online",
      "flags": 0,
      "memory-min-mb": 64,
      "memory-max-mb": 1024,
      "redo-read-sleep": 10000,
      "arch-read-sleep": 10000000,
      "checkpoint-interval": 10,
      "event-table": "SYSTEM.OPENLOGREPLICATOR",
      "tables": [
        {"table": "OWNER1.TABLENAME1", "key": "col1, col2, col3"},
        {"table": "OWNER1.TABLENAME2"},
        {"table": "OWNER2.TAB%"}
      ]
    }
  ],
  "targets": [
    {
      "alias": "K1", 
      "source": "S1",
      "writer": {
        "type": "kafka",
        "topic": "O112A",
        "brokers": "localhost:9092",
        "short-message": 0,
        "max-message-mb": 500,
        "max-messages": 200000
      }
    }
  ]
}
